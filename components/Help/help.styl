  justify-content center

.facp
  display flex
  align-items center
  cursor pointer 

.as-flex-end
  align-self flex-end

.openBGColor
  background-color #F8F8F8
.selectedBG
  background-color #45689A
.whiteBG
  background-color  #ffffff
.fwBolder
  font-weight 800

.whiteColor
  color #ffffff
.blackColor
  color #000000
.highlightColor
  color #44689A
.greyColor
  color #848484

.overflow-hidden
  overflow hidden
.overflow-auto
  overflow auto

.ht-auto
  height auto
.ht-0
  height 0
.ht-45
  height 45px
.ht-110
  height 110px
.ht-175
  height 175px

.wh-250
  width 250px
.wh-200
  width 200px

.fs-11p
  font-size 11px
.fs-10p
  font-size 10px
.fs-12p
  font-size 12px
.fs-13p
  font-size 13px
