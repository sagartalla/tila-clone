@import '../../../../layout/main/executables.styl';

.four-not-four-main
    background-image radial-gradient(circle at 48% 50%, #d8e1e8, generic-gray-color)
    height 100vh
    width 100%
    margin-top -5%
    .four-not-four-inn
        display flex
        align-items center
        justify-content center
        height 100%
        flex-direction column
        background: rgb(249,249,249);
        background: linear-gradient(0deg, rgba(249,249,249,1) 52%, rgba(216,225,232,1) 52%);
        .landing-page-btn:focus
            text-decoration none

        .four-not-four-no
            text-align center
            color light-gray-clr-transparent
            font-size 300px

        .fr-nt-fr-img
            box-shadow 0 0 68px 0 rgba(0, 0, 0, 0.04)
            background-color white
            height 416px
            width 416px
            border-radius 50%

            .fr-nt-fr-img-main
                padding 5px
                border-radius 50%
                display flex
                align-items center
                justify-content center
                a
                    width 100%
                    height 100%
                img
                    width 100%
                    height 100%
                    object-fit scale-down
                &:hover
                    box-shadow: 0 2px 4px 0 rgba(165, 165, 165, 0.5);
