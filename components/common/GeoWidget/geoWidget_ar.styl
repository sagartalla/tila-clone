@import './geoWidget.styl'
.auto-suggestions-wrap
  .delete-btn
    width 25px
    height 25px
    border-radius 50%
    background #ccc
    text-align center
    padding 2px
    position absolute
    top 0
    left 0
    cursor pointer
    display none
  &:hover
      .delete-btn
        display block
