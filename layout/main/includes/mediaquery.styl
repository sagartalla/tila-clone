@import '../executables.styl';

@media screen and (min-device-width: 768px) and (max-device-width: 1024px) and (orientation : portrait){
  .ipad-pl-0 {
    padding-left 0
  }

  .ipad-pl-15 {
    padding-left 15px
  }

  .ipad-tp-5 {
    padding-top 5px
  }

  .ipad-tb-5 {
    padding-bottom 5px
  }

  .bdr-lt {
    border-left 1px solid rgba(151, 151, 151, 0.4)
  }

  .ipad-pr-15 {
    padding-right 15px
  }

  .ipad-fs-14 {
    font-size 14px
  }

  .ipad-fs-20 {
    font-size 20px
  }

  .ipad-p-0 {
    padding 0
  }

  .ipad-pr-0 {
    padding-right 0
  }

  .ipad-br-0 {
    border-right 0 !important
  }

  .ipad-mb-15 {
    margin-bottom 15px
  }

  .ipad-flex-clm {
    flex-direction column
  }

  .ipad-flex-wrp {
    flex-wrap wrap
  }
}

@media screen and (min-device-width: 768px) and (max-device-width: 1024px) and (orientation : landscape){
    .footer-social-list-main-inn {
          .footer-sc-img {
            width 100%
            height 100%
          }
        
    }
      
}